    "url": "https://github.com/warwickgrigg/ezform/issues"
  },
  "homepage": "https://github.com/warwickgrigg/ezform#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "eslint": "^6.8.0",
    "jest": "^26.1.0",
    "microbundle": "^0.12.3",
    "serve": "^11.3.2",
    "vhtml": "^2.2.0"
  },
  "dependencies": {
    "react": "file:vhtml-react"
