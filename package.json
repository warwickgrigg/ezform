{
  "name": "ezform",
  "version": "0.1.1",
  "description": "easy form",
  "source": "src/ezform.js",
  "main": "dist/ezform.js",
  "module": "dist/ezform.module.js",
  "umd:main": "dist/ezform.umd.js",
  "esmodule": "dist/ezform.modern.js",
  "files": [
    "dist/"
  ],
  "type": "module",
  "scripts": {
    "build": "microbundle --css-modules false --globals react=React ",
